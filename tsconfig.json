{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "common/*": ["src/components/common/*"],
      "components/*": ["src/components/*"],
      "constants/*": ["src/constants/*"],
      "services/*": ["src/services/*"],
      "utilities/*": ["src/utilities/*"],
    }
  },
  "exclude": [
    "node_modules"
  ]
}